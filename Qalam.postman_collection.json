		{
			"name": "Teacher Registration (4 Steps)",
			"description": "Complete teacher registration flow with phone verification, OTP, personal info, and document upload",
			"item": [
				{
					"name": "Step 1: Send OTP",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    const response = pm.response.json();",
									"    if (response.Succeeded) {",
									"        console.log('✓ OTP sent successfully');",
									"    }",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"countryCode\": \"+966\",\n  \"phoneNumber\": \"503333256\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/Api/V1/Authentication/Teacher/Step1-SendOtp",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"Api",
								"V1",
								"Authentication",
								"Teacher",
								"Step1-SendOtp"
							]
						},
						"description": "Send OTP code to teacher's phone number. No authentication required."
					},
					"response": []
				},
				{
					"name": "Step 2: Verify OTP & Create Account",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    const response = pm.response.json();",
									"    if (response.Succeeded && response.Data) {",
									"        // Save token for next steps",
									"        pm.environment.set('teacher_token', response.Data.Token);",
									"        console.log('✓ Account created, token saved');",
									"        ",
									"        // Display next step info",
									"        if (response.Data.NextStep) {",
									"            console.log('Next Step:', response.Data.NextStep.NextStepName);",
									"            console.log('Current Step:', response.Data.NextStep.CurrentStep);",
									"        }",
									"    }",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"phoneNumber\": \"503333256\",\n  \"otpCode\": \"1234\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/Api/V1/Authentication/Teacher/Step2-VerifyOtp",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"Api",
								"V1",
								"Authentication",
								"Teacher",
								"Step2-VerifyOtp"
							]
						},
						"description": "Verify OTP code and create basic teacher account. Returns JWT token and next step information."
					},
					"response": []
				},
				{
					"name": "Step 3: Complete Personal Info",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    const response = pm.response.json();",
									"    if (response.Succeeded && response.Data) {",
									"        // Update token if provided",
									"        if (response.Data.Account && response.Data.Account.Token) {",
									"            pm.environment.set('teacher_token', response.Data.Account.Token);",
									"        }",
									"        console.log('✓ Personal info completed');",
									"        ",
									"        // Display next step",
									"        if (response.Data.NextStep) {",
									"            console.log('Next Step:', response.Data.NextStep.NextStepName);",
									"        }",
									"    }",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"auth": {
							"type": "bearer",
							"bearer": [
								{
									"key": "token",
									"value": "{{teacher_token}}",
									"type": "string"
								}
							]
						},
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"firstName\": \"Ahmed\",\n  \"lastName\": \"Ali\",\n  \"email\": \"ahmed.ali@example.com\",\n  \"password\": \"Pass@123456\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/Api/V1/Authentication/Teacher/Step3-PersonalInfo",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"Api",
								"V1",
								"Authentication",
								"Teacher",
								"Step3-PersonalInfo"
							]
						},
						"description": "Complete teacher personal information (name, email, password). Requires Bearer token from Step 2. UserId is automatically extracted from token - DO NOT send in request body."
					},
					"response": []
				},
				{
					"name": "Step 4: Upload Documents",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    const response = pm.response.json();",
									"    if (response.Succeeded) {",
									"        console.log('✓ Documents uploaded successfully');",
									"        console.log('Registration complete - awaiting admin approval');",
									"    }",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"auth": {
							"type": "bearer",
							"bearer": [
								{
									"key": "token",
									"value": "{{teacher_token}}",
									"type": "string"
								}
							]
						},
						"method": "POST",
						"header": [],
						"body": {
							"mode": "formdata",
							"formdata": [
								{
									"key": "IsInSaudiArabia",
									"value": "true",
									"type": "text"
								},
								{
									"key": "IdentityType",
									"value": "1",
									"description": "1=NationalId, 2=Iqama, 3=Passport",
									"type": "text"
								},
								{
									"key": "DocumentNumber",
									"value": "1234567890",
									"type": "text"
								},
								{
									"key": "IdentityDocumentFile",
									"description": "Upload identity document (PDF/Image)",
									"type": "file",
									"src": []
								},
								{
									"key": "Certificates[0].File",
									"description": "Upload certificate file",
									"type": "file",
									"src": []
								},
								{
									"key": "Certificates[0].Title",
									"value": "Bachelor Degree in Education",
									"type": "text"
								},
								{
									"key": "Certificates[0].Issuer",
									"value": "King Saud University",
									"type": "text"
								},
								{
									"key": "Certificates[0].IssueDate",
									"value": "2020-06-15",
									"type": "text"
								}
							]
						},
						"url": {
							"raw": "{{base_url}}/Api/V1/Authentication/Teacher/Step4-UploadDocuments",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"Api",
								"V1",
								"Authentication",
								"Teacher",
								"Step4-UploadDocuments"
							]
						},
						"description": "Upload teacher identity documents and certificates. Requires Bearer token and Teacher role. UserId is automatically extracted from token."
					},
					"response": []
				}
			]
